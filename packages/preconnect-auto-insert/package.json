{
  "name": "@fast-tools/preconnect-auto-insert",
  "version": "1.0.0",
  "description": "A webpack plugin for inserting preconnect and dns-prefetch link tags automatically",
  "main": "lib/preconnect.cjs.js",
  "module": "lib/preconnect.esm.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build:clean": "rm -rf ./lib",
    "build:lib": "pnpm build:clean && rollup -c rollup.config.js",
    "test": "vitest"
  },
  "keywords": [
    "webpack 5 plugin",
    "preconnect",
    "dns-prefetch",
    "FP",
    "website optimize"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moonliujk/fast-tools.git"
  },
  "author": "Leslie <https://github.com/Moonliujk>",
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "exports": {
    "types": "./lib/src/index.d.ts",
    "import": "./lib/preconnect.esm.js",
    "default": "./lib/preconnect.cjs.js"
  },
  "bugs": {
    "url": "https://github.com/Moonliujk/fast-tools/issues"
  },
  "homepage": "https://github.com/Moonliujk/fast-tools",
  "peerDependencies": {
    "webpack": "^5"
  },
  "dependencies": {
    "node-html-parser": "^6.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/webpack": "^5.28.5",
    "webpack": "^5.93.0"
  },
  "files": [
    "lib"
  ]
}
